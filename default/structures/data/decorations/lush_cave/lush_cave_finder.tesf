// finds a spawn location for a lush cave.
id "lush_cave_finder";

num height = originY();

while(height > 0 && getBlock(0, height-originY(), 0) != "minecraft:air") {
    height = height - 1;
}

while(height > 0 && getBlock(0, height-originY(), 0) == "minecraft:air") {
    height = height - 1;
}

if(height <= 1) return;

structure(0,(height-originY()),0,"lush_cave","NONE");